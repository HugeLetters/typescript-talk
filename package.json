{
  "name": "ts-talk",
  "type": "module",
  "scripts": {
    "emit-pdf": "mkdir -p ./build && cat main.md | md-to-pdf --css=\"$(cat ./main.css | tr -d '\n')\" > ./build/main.pdf",
    "check-pdf": "npm run emit-pdf && open ./build/main.pdf"
  },
  "dependencies": {
    "md-to-pdf": "^5.2.4",
    "typescript": "^5.4.2"
  }
}
